<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>Account</servlet-name>
        <servlet-class>controller.Account</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewMenteeProfile</servlet-name>
        <servlet-class>controller.ViewMenteeProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewAllMentor</servlet-name>
        <servlet-class>controller.ViewAllMentor</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Account</servlet-name>
        <url-pattern>/Account</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewMenteeProfile</servlet-name>
        <url-pattern>/ViewMenteeProfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewAllMentor</servlet-name>
        <url-pattern>/ViewAllMentor</url-pattern>
    </servlet-mapping>
</web-app>
